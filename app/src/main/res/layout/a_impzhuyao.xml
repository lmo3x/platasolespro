        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_above="@+id/main_ll_bottom"
        android:background="@color/sezhi_argb_D7D7D7" />

    <LinearLayout
        android:id="@+id/main_ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:background="@color/sezhi_argb_F4F4F4"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/menutab_sfjT12jf_one"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/menutab_stupianf_one"
                android:layout_width="33dp"
                android:layout_height="29dp"
                android:src="@mipmap/useimg_gdhgfasrg" />

            <TextView
                android:id="@+id/menutab_swenzif_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:gravity="center"
                android:text="@string/zhuyetab_forintudai_inci"
                android:textColor="@color/sezhi_argb_444444"
                android:textSize="14dp"
                android:visibility="gone" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/menutab_sfjT12jf_dingdna"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/menutab_stupianf_dingdna"
                android:layout_width="24dp"
                android:layout_height="27dp"
                android:src="@mipmap/useimg_rtyjhdandanhui" />

            <TextView
                android:id="@+id/menutab_swenzif_menutab_sfjT12jf_dingdna"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:gravity="center"
                android:text="@string/peizhiandout_egdsgj_per"
                android:textColor="@color/sezhi_argb_444444"
                android:textSize="14dp"
                android:visibility="gone" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/menutab_sfjT12jf_two"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/menutab_stupianf_two"
                android:layout_width="29dp"
                android:layout_height="29dp"
                android:src="@mipmap/useimg_rtyjhtgrfes" />

            <TextView
                android:id="@+id/menutab_swenzif_two"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:gravity="center"
                android:text="@string/peizhiandout_egdsgj_per"
                android:textColor="@color/sezhi_argb_444444"
                android:textSize="14dp"
                android:visibility="gone" />

        </LinearLayout>
    </LinearLayout>
</RelativeLayout>