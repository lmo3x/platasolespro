                        <TextView
                            android:id="@+id/liangliang_wenTTVana_Titlbiaoshibailes35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="85dp"
                            android:gravity="center"
                            android:textColor="@color/sezhi_argb_333333"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:text="@string/zhuyetab_fangfail_pre" />


                        <TextView
                            android:id="@+id/liangliang_wenTTVana_bailes35"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/liangliang_wenTTVana_Titlbiaoshibailes35"
                            android:layout_centerHorizontal="true"
                            android:layout_marginHorizontal="54dp"
                            android:layout_marginTop="22dp"
                            android:gravity="center"
                            android:lineSpacingMultiplier="1.2"
                            android:textColor="@color/sezhi_argb_333333"
                            android:textSize="12dp"
                            tools:text="@string/zhuyetab_fangfail_los" />

                    </RelativeLayout>

                    <TextView
                        android:visibility="gone"
                        android:id="@+id/str_actualizar_receptora"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:layout_below="@+id/str_fecha_aplicacion_Ll"
                        android:layout_marginHorizontal="30dp"
                        android:layout_marginTop="25dp"
                        android:layout_marginBottom="13dp"
                        android:background="@drawable/bj_userallow_yes"
                        android:gravity="center"
                        android:text="@string/zhuyetab_fangfail_act"
                        android:textColor="@color/sezhi_argb_baise"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <com.youth.banner.Banner
                        android:id="@+id/lunzhuantu_djfsljaf_fjfjTjjt233"
                        android:layout_width="match_parent"
                        android:layout_height="140dp"
                        android:layout_margin="20dp" />
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/str_aviso_privacidadLl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="24dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@mipmap/useimg_hbgfeasgef" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:text="@string/jingogojian_dibugonggong_por"
            android:textColor="@color/sezhi_argb_heise"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/jingojian1dibuxieyi"
            android:layout_width="wrap_content"
